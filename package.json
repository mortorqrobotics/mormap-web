{
    "name": "morteam-web",
    "version": "0.0.1",
    "description": "The new web client for MorTeam, the FRC collaboration tool",
    "scripts": {
        "build": "webpack --config ./config/webpack.config.dev.js",
        "watch": "webpack --config ./config/webpack.config.dev.js --watch",
        "build:prod": "webpack --config ./config/webpack.config.prod.js",
        "watch:prod": "webpack --config ./config/webpack.config.prod.js --watch",
        "gulp": "webpack",
        "post-install": "npm run build",
        "trailing": "find src -name '*.js' -o -name '*.json' | xargs -n1 sed -i -e 's/\\s\\+$//'"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mortorqrobotics/morteam-web.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/mortorqrobotics/morteam-web/issues"
    },
    "homepage": "https://github.com/mortorqrobotics/morteam-web",
    "dependencies": {
        "autosize": "^4.0.0",
        "axios": "^0.17.1",
        "dompurify": "^1.0.3",
        "jquery": "^3.2.1",
        "postcss-loader": "^2.0.10",
        "radium": "^0.21.1",
        "react": "^15.1.0",
        "react-bootstrap": "^0.32.0",
        "react-context-menus": "^1.0.9",
        "react-dev-utils": "^5.0.0",
        "react-dom": "^16.2.0",
        "react-google-maps": "^9.4.5",
        "react-masonry-component": "^6.0.2",
        "react-modal": "^3.1.11",
        "react-redux": "^5.0.6",
        "redux": "^3.5.2",
        "redux-sounds": "^1.1.1",
        "redux-thunk": "^2.1.0",
        "socket.io-client": "^2.0.4"
    },
    "devDependencies": {
        "autodll-webpack-plugin": "^0.3.8",
        "autoprefixer": "^7.2.5",
        "babel-core": "^6.10.4",
        "babel-eslint": "^8.2.1",
        "babel-loader": "^7.1.2",
        "babel-plugin-syntax-async-functions": "^6.8.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-object-rest-spread": "^6.8.0",
        "babel-plugin-transform-regenerator": "^6.9.0",
